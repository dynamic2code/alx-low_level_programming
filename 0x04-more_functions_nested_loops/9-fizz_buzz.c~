#include <stdio.h>
/**
 *main -prints the numbers from 1 to 100
 *for multiples of three print Fizz
 *for the multiples of five print Buzz
 *for multiples of both three and five print FizzBuzz
 */

int is_divisible_by_3(int x);
int is_divisible_by_5(int x);

int main(void)
{
int i;
 
for (i = 1; i <= 100; i++)
{
if (is_divisible_by_3(i))
{
printf("Fizz"0;
}
else if(is_divisible_by_5(i))
{
printf("Buzz");
}
else if(is_divisible_by_5(i) && is_divisible_by_3(i))
{
printf("FizzBuzz");
}
else
{
 printf(i);
}
}  
}

int is_divisible_by_3(int x)
{
if (x % 3 == 0)
{
return (1);
}
else
{
return (0);
}

int is_divisible_by_5(int x)
{

}
